@page "/Dashboards/DoctorDashboard"
@model MedicalWebApp.Pages.Dashboards.DoctorDashboardModel

<h2>Welcome, Doctor!</h2>
<p>This is your dashboard. View patient appointments, medical records, and more.</p>

<div class="container">

    <main role="main">
        <div class="square-container">
            <div class="buttons-container">
                <h3> Navigation </h3>
                <button onclick="location.href='@Url.Action("Index", "Doctor_Appointment")'" class="custom-button" type="button">Check out upcoming Appointments</button>
                <button onclick="location.href='@Url.Action("Index", "Patient_List_For_Doctor")'" class="custom-button" type="button">Patient List</button>

            </div>
            <div class="square"></div>
        </div>
    </main>
</div>
